{
  "version": 2,
  "name": "collabdocs",
  "buildCommand": "npm install --prefix backend",
  "devCommand": "cd backend && node server-demo.js",
  "installCommand": "npm install --prefix backend",
  "framework": null,
  "outputDirectory": ".",
  "builds": [
    {
      "src": "backend/server-demo.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "demo.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/server-demo.js"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "backend/server-demo.js"
    },
    {
      "src": "/",
      "dest": "/demo.html"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|json))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/demo.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
